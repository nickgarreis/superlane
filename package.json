{
  "name": "@figma/my-make-file",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint:checks": "node scripts/quality/check-suppressions.mjs",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"convex/**/*.ts\" \"vite.config.ts\" && npm run lint:checks",
    "typecheck:frontend": "tsc --noEmit --project tsconfig.app.json && tsc --noEmit --project tsconfig.node.json",
    "typecheck:backend": "convex codegen && tsc --noEmit --project convex/tsconfig.json",
    "typecheck": "npm run typecheck:frontend && npm run typecheck:backend",
    "test": "vitest run",
    "test:backend": "vitest run convex/__tests__",
    "test:frontend": "vitest run src/app",
    "test:frontend:coverage": "vitest run src/app --coverage",
    "test:rbac": "vitest run convex/__tests__",
    "perf:check": "node scripts/performance/check-budgets.mjs && node scripts/quality/check-bundle-budgets.mjs",
    "perf:report": "node scripts/performance/check-budgets.mjs --report-only && node scripts/quality/check-bundle-budgets.mjs --report-only",
    "security:env": "node scripts/security/check-env.mjs",
    "security:urls": "node scripts/security/check-workos-urls.mjs --all",
    "security:urls:strict": "node scripts/security/check-workos-urls.mjs --all --strict-placeholders",
    "security:secrets": "node scripts/security/check-secrets.mjs",
    "security:deps": "node scripts/security/check-dependencies.mjs",
    "security:check": "npm run security:env && npm run security:urls && npm run security:secrets && npm run security:deps",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy"
  },
  "dependencies": {
    "@convex-dev/workos": "^0.0.1",
    "@convex-dev/workos-authkit": "^0.1.6",
    "@emotion/react": "11.14.0",
